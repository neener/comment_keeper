<form method="POST" action="/comments">
	<input name="video_id">

	<input name="authenticity_token" value="<%= form_authenticity_token() %>" type="hidden">
	
	<button>Submit</button>
	<ul>
	<% @comments.each do |comment|%>
	<li><%= comment.text %></li>
	<button>Add</button>
	<% end %>
	</ul>
</form>